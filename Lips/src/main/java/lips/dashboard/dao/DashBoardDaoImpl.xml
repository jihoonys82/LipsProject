<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="lips.dashboard.dao.DashBoardDao">
	<select id="seldashboardDashDto"
		parameterType="lips.dashboard.dto.DashBoardDto"
		resultType="lips.dashboard.dto.DashBoardDto">
		SELECT*FROM dashboard WHERE userId=#{userId} AND projectId=#{projectId}
	</select>
	
	<select id="selAssetMaxXDto" parameterType="lips.dashboard.dto.DashBoardDto" resultType="Integer">
		SELECT MAX (positionX) FROM Dashboard_user_asset WHERE DashboardId =#{dashboardId}
	</select>

</mapper>